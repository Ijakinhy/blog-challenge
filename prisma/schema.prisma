generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model BlogChallenge {
  id          Int      @id @default(autoincrement())
  title       String?
  description String?
  created_at  DateTime @default(now()) @db.Timestamptz(6)
  post_image  String?
  own_id      String?  @default(dbgenerated("gen_random_uuid()")) @db.Uuid

  @@map("blog_challenge")
}
